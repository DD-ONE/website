---
import { checkSession } from "../db/session/checkSession";
import FormLogin from "../components/RegisterAndLogin.svelte";
import BaseLayout from "../layouts/BaseLayout.astro";

const isLoggedin = await checkSession(Astro);
if (isLoggedin) return Astro.redirect("/");
---

<BaseLayout>
  <div class="wrapper">
    <FormLogin client:load mode="login" />
  </div>
</BaseLayout>
